    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "dayjs": "^1.11.10",
    "electron": "^28.1.3",
    "element-plus": "^2.4.4",
    "eslint": "^8.56.0",
    "less": "^4.2.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
